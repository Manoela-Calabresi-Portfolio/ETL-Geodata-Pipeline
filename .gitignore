# Deletar e recriar
rm .gitignore

cat > .gitignore << 'EOF'
# Large files only
old/
stuttgart-etl/data/raw/baden-wuerttemberg-latest.osm.pbf
stuttgart-etl/data/staging/*.parquet
stuttgart-etl/data/duckdb/*.duckdb
stuttgart-etl/data/exports/*.geojson
stuttgart-etl/data/maps/*.png

# Python cache
__pycache__/
*.pyc
# AI assistant prompt
stuttgart-etl/config/ai_assistant_prompt.md
EOF